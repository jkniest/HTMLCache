{
    "name": "jkniest/htmlcache",
    "description": "A simple HTML Cache middleware for laravel",
    "type": "library",
    "require": {
        "laravel/framework": "^5.4|^5.5|^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "orchestra/testbench": "^3.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Kniest",
            "email": "mail@jkniest.de"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JKniest\\HtmlCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JKniest\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JKniest\\HtmlCache\\HtmlCacheServiceProvider"
            ]
        }
    }
}
